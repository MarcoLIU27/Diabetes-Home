<script>
        function showModel(){
            var model = document.getElementById("successful-update-data");
            model.style.display = "block";
        }

        function closeModel(){
            var model = document.getElementById("successful-update-data");
            model.style.display = "none";
        }

        function disableScroll(){
            $("body").attr("id","disable");  
        }

        function enableScroll(){
            $("body").removeAttr("id","disable");
        }
</script>


<body>
    {{{body}}}
</body>


